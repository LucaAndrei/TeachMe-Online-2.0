{
    "name": "teachme-backend",
    "version": "0.0.0",
    "description": "teachme node backend",
    "main": "app.js",
    "scripts": {
        "mongo:start": "docker run --name realworld-mongo -p 27017:27017 mongo & sleep 5",
        "start": "node ./app.js",
        "dev": "nodemon ./app.js",
        "stop": "lsof -ti :3000 | xargs kill",
        "mongo:stop": "docker stop realworld-mongo && docker rm realworld-mongo"
    },
    "private": true,
    "dependencies": {
        "body-parser": "1.20.1",
        "cors": "2.7.1",
        "ejs": "2.4.1",
        "errorhandler": "1.4.3",
        "express": "4.18.2",
        "express-jwt": "3.3.0",
        "express-session": "1.13.0",
        "jsonwebtoken": "7.1.9",
        "method-override": "2.3.10",
        "methods": "1.1.2",
        "mongoose": "5.7.5",
        "mongoose-unique-validator": "1.0.2",
        "morgan": "1.9.1",
        "passport": "0.3.2",
        "passport-local": "1.0.0",
        "postmark": "^2.3.0",
        "request": "2.88.2",
        "slug": "0.9.1",
        "socket.io": "^2.3.0",
        "underscore": "1.8.3"
    },
    "devDependencies": {
        "nodemon": "^1.11.0"
    }
}
